{
  "name": "pos-app",
  "version": "0.1.0",
  "main": "./src/main.ts",
  "description": "Simple Point-of-sale application",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "yarn start:renderer",
    "start:main": "cross-env NODE_ENV=development ./node_modules/.bin/electron -r ts-node/register/transpile-only .",
    "start:preload": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./configs/webpack.config.preload.ts",
    "start:renderer": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack serve --config ./configs/webpack.config.dev.ts",
    "build": "TS_NODE_TRANSPILE_ONLY=true webpack --config ./configs/webpack.config.ts",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chanoonna/pos-app.git"
  },
  "author": "Chanoon Na, Haijoon Kim",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chanoonna/pos-app/issues"
  },
  "homepage": "https://github.com/chanoonna/pos-app#readme",
  "lint-staged": {
    "src/**/*.{js,jsx,json,ts,tsx}": [
      "yarn eslint",
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.11.9",
    "@mui/material": "^5.11.10",
    "@mui/styled-engine-sc": "^5.11.9",
    "@types/lodash": "^4.14.191",
    "classnames": "^2.3.2",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.2.10",
    "@svgr/webpack": "^6.5.1",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/sqlite3": "^3.1.8",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "electron": "^23.1.0",
    "electron-builder": "^23.6.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
